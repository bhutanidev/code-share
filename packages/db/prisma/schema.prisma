generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id        String    @id @default(uuid())
  email     String
  name      String
  password  String
  rooms     Room[]
}

model Room{
  id        Int        @id @default(autoincrement())
  slug      String     @unique
  createdAt DateTime   @default(now())
  adminId   String
  admin     User       @relation(fields: [adminId] , references: [id])
}
